{
  "name": "concept-visualizer-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:specific": "vitest run --testNamePattern",
    "test:file": "vitest run --config vitest.config.ts",
    "test:e2e": "playwright test",
    "test:visual": "playwright test -c visual-regression.config.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:visual:ui": "playwright test -c visual-regression.config.ts --ui",
    "test:e2e:update": "playwright test --update-snapshots",
    "test:visual:update": "playwright test -c visual-regression.config.ts --update-snapshots",
    "test:a11y": "playwright test -c accessibility.config.ts",
    "test:a11y:ui": "playwright test -c accessibility.config.ts --ui"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.1",
    "@mui/material": "^7.0.1",
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-query": "^5.72.0",
    "@types/js-cookie": "^3.0.6",
    "@types/uuid": "^10.0.0",
    "axios": "^1.8.4",
    "js-cookie": "^3.0.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^6.22.1",
    "supabase": "^2.20.5",
    "use-context-selector": "^2.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.7.0",
    "@playwright/test": "^1.51.1",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.31",
    "postcss-import": "^16.1.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "vitest": "^1.2.1"
  }
}
