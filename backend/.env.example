# API settings
CONCEPT_API_PREFIX=/api

# CORS settings
# For development
CONCEPT_CORS_ORIGINS=["http://localhost:3000", "http://localhost:5173"]
# For production (uncomment and replace with your production domain)
# CONCEPT_CORS_ORIGINS=["https://your-production-domain.com"]

# JigsawStack API settings
CONCEPT_JIGSAWSTACK_API_KEY=your_jigsawstack_api_key
CONCEPT_JIGSAWSTACK_API_URL=https://api.jigsawstack.com

# Logging settings
# Use INFO for development, WARNING or ERROR for production
CONCEPT_LOG_LEVEL=INFO

# Supabase settings
CONCEPT_SUPABASE_URL=https://your-project-id.supabase.co
CONCEPT_SUPABASE_KEY=your_supabase_api_key

# Environment settings
# Can be 'development' or 'production'
# This affects secure cookies and other security settings
CONCEPT_ENVIRONMENT=development 